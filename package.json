{
	"name": "website",
	"type": "module",
	"private": true,
	"scripts": {
		"format": "prettier . --write --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss",
		"lint": "prettier . --check --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss && eslint .",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@nathanlindsay/prettier-config": "^0.0.1",
		"@playwright/test": "^1.39.0",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.27.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"postcss": "^8.4.31",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"svelte": "^4.2.7",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.11"
	},
	"dependencies": {
		"lucide-svelte": "^0.282.0"
	}
}
